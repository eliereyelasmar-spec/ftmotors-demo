  "apiInstructions": {
    "authentication": {
      "method": "Private App Access Token",
      "steps": [
        "1. Go to HubSpot Settings > Integrations > Private Apps",
        "2. Create new Private App: 'Fast Track Motors API'",
        "3. Request scopes: crm.objects.contacts.read, crm.objects.contacts.write, crm.objects.deals.read, crm.objects.deals.write, crm.schemas.contacts.read, crm.schemas.contacts.write, crm.schemas.deals.read, crm.schemas.deals.write",
        "4. Copy Access Token for use in API calls"
      ]
    },
    "createPropertyGroups": {
      "endpoint": "POST https://api.hubapi.com/crm/v3/properties/{objectType}/groups",
      "headers": {
        "Authorization": "Bearer YOUR_ACCESS_TOKEN",
        "Content-Type": "application/json"
      },
      "examplePayload": {
        "name": "dealership_info",
        "label": "Dealership Information",
        "displayOrder": 0
      },
      "note": "Create all 3 property groups first before creating properties"
    },
    "createProperty": {
      "endpoint": "POST https://api.hubapi.com/crm/v3/properties/{objectType}",
      "headers": {
        "Authorization": "Bearer YOUR_ACCESS_TOKEN",
        "Content-Type": "application/json"
      },
      "examplePayloadText": {
        "name": "vehicle_interest",
        "label": "Vehicle Interest",
        "type": "string",
        "fieldType": "text",
        "groupName": "dealership_info",
        "description": "Specific vehicle(s) the customer is interested in",
        "hasUniqueValue": false,
        "hidden": false
      },
      "examplePayloadDropdown": {
        "name": "lead_source",
        "label": "Lead Source",
        "type": "enumeration",
        "fieldType": "select",
        "groupName": "dealership_info",
        "description": "How the customer found Fast Track Motors",
        "hasUniqueValue": false,
        "hidden": false,
        "options": [
          {
            "label": "Walk-In",
            "value": "walk_in",
            "displayOrder": 0,
            "hidden": false
          },
          {
            "label": "Phone Call",
            "value": "phone_call",
            "displayOrder": 1,
            "hidden": false
          }
        ]
      },
      "note": "Replace {objectType} with 'contacts' or 'deals' depending on property"
    },
    "n8nWorkflowUsage": {
      "description": "Use this JSON structure in n8n HTTP Request nodes to create properties programmatically",
      "steps": [
        "1. Create n8n workflow with HTTP Request node",
        "2. Method: POST",
        "3. URL: https://api.hubapi.com/crm/v3/properties/contacts (or deals)",
        "4. Authentication: Header Auth (Authorization: Bearer YOUR_TOKEN)",
        "5. Body: JSON from contactProperties or dealProperties arrays",
        "6. Loop through each property in array",
        "7. Execute requests sequentially"
      ]
    },
    "bulkCreateScript": {
      "description": "Example Node.js script to bulk create all properties",
      "code": "See /crm/scripts/bulk-create-properties.js (to be created if needed)"
    }
  }
}
